{
  "version": 2,
  "builds": [
    {
      "src": "api/create-checkout-session.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/health.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/create-checkout-session",
      "dest": "/api/create-checkout-session.js",
      "methods": ["POST"]
    },
    {
      "src": "/health",
      "dest": "/api/health.js",
      "methods": ["GET"]
    }
  ]
}
